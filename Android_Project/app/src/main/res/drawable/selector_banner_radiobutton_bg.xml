<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--Banner小圆点图形选择器-->
    <item android:state_checked="true">
        <shape>
            <size android:width="@dimen/ad_radio_size" android:height="@dimen/ad_radio_size" />
            <solid android:color="@color/ad_orange_dot" />
            <corners android:radius="@dimen/ad_radio_size" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <size android:width="@dimen/ad_radio_size" android:height="@dimen/ad_radio_size" />
            <solid android:color="@android:color/white" />
            <corners android:radius="@dimen/ad_radio_size" />
        </shape>
    </item>
</selector>